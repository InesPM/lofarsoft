generators = 'calibrators' calibrator/field; default=calibrator 
gencode = 'list' list/file, default=list, primary gens from gaussian list or file 
primarygen = 'file.name' filename for primary gens if gencode=file, x,y,snr 
over-ny = 2.0 factor of nyquist sample for binning bmaj etc vs snr; default=2
kappa-bin = 3.0 kappa for clipping within each bin; default=3.0
kappa-all = 1.0 kappa for clipping for analytic fit; default=3.0
snrcut = 10.0 minimum snr for statistics; default=10.0
snrtop = 0.10 fraction of SNR>snrcut as primary generators; default=0.10
snrbot = 0.30 fraction of SNR>snrcut as all generators; default=0.30
snrcutstack = 15.0 Unresolved sources with higher SNR taken for stacked psfs; default=15.0
tess_method = 0 0=normal, 1=0+round, 2=LogSNR, 3=SqrtLogSNR ; default=0
tess_sc = 's' (s)imple/(c)omplicated - normal or approximate (fuzzy); default=s 
tess_fuzzy = 0.05 fraction of overlap for fuzzy tesselation; default=0.05
