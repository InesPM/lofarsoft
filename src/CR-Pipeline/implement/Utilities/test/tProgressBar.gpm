set grid
set xlabel 'Iteration number'

set term post color solid

set ylabel 'Running time [s]'
set out 'runningTime.ps'
plot 'timer1.data' u 1:3 t 'clock' w lp, 'timer2.data' u 1:3 t 'timeval' w lp

set ylabel 'Expected total time [sec]'
set out 'totalTime.ps'
plot 'timer1.data' u 1:4 t 'clock' w lp, 'timer2.data' u 1:4 t 'timeval' w lp

set ylabel 'Remain time [sec]'
set out 'remainTime.ps'
plot 'timer1.data' u 1:5 t 'clock' w lp, 'timer2.data' u 1:5 t 'timeval' w lp

