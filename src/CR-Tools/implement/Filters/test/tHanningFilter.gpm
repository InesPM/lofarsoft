
set term post color solid
set output 'tHanningFilter.ps'

##______________________________________________________________________________
## Plot the filter weights for different values of alpha

set grid
set xlabel "Input channel"
set ylabel "Filter weights"

plot 'tHanningFilter_alpha.data' u 1:2 w l t "alpha=0.40, beta=0",\
	'tHanningFilter_alpha.data' u 1:3 w l t "alpha=0.50, beta=0",\
	'tHanningFilter_alpha.data' u 1:4 w l t "alpha=0.60, beta=0",\
	'tHanningFilter_alpha.data' u 1:5 w l t "alpha=0.70, beta=0"

##______________________________________________________________________________
## Surface plot for range of values of alpha

set xlabel "alpha"
set ylabel "Input channel number"
set zlabel "Filter weights"

set contour base 

splot 'tHanningFilter_alphaScan.data' u 1:2:3 w l

##______________________________________________________________________________

set xlabel "Input channel"
set ylabel "Filter weights"

plot 'tHanningFilter_beta.data' u 1:2 w l t "beta=blocksize/2",\
	'tHanningFilter_beta.data' u 1:3 w l t "beta=blocksize/4"

##______________________________________________________________________________

exit
