
set (CMAKE_INSTALL_PREFIX  ${MATHGL_prefix} CACHE PATH "Installation prefix" FORCE)
set (enable-python         ${MATHGL_ENABLE_PYTHON} CACHE BOOL "Enable Python extension")
set (enable-qt             ${MATHGL_ENABLE_QT} CACHE BOOL "Enable Qt extension")
set (QT4_FOUND             TRUE CACHE BOOL "Found Qt4?")
set (QT_QTCORE_LIBRARY     ${QT_QTCORE_LIBRARY} CACHE FILEPATH "Path to libqtcore")
set (QT_QTGUI_LIBRARY      ${QT_QTGUI_LIBRARY} CACHE FILEPATH "Path to libqtgui")
set (QT_QTDESIGNER_LIBRARY ${QT_QTDESIGNER_LIBRARY} CACHE FILEPATH "Path to libqtdesigner")
set (QT_MOC_EXECUTABLE     ${QT_MOC_EXECUTABLE} CACHE FILEPATH "Path to moc")
set (QT_QTCORE_INCLUDE_DIR ${QT_QTCORE_INCLUDE_DIR} CACHE PATH "Path to include/QtCore")
set (QT_QTGUI_INCLUDE_DIR  ${QT_QTGUI_INCLUDE_DIR} CACHE PATH "Path to include/QtGui")
set (QT_INCLUDE_DIR        ${QT_INCLUDE_DIR} CACHE PATH "Path to Qt header files")

include_directories (
  ${QT_QTCORE_INCLUDE_DIR}
  ${QT_QTGUI_INCLUDE_DIR}
)
